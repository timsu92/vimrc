{
	"$schema": "https://puremourning.github.io/vimspector/schema/vimspector.schema.json#",
	"launch": {
		"adapter": "CodeLLDB",
		"filetypes": [
			"cpp",
			"c",
			"objc",
			"rust"
		],
		"configuration": {
			"name": "launch",
			"request": "launch",
			"program": "${workspaceRoot}/a.out",
			"args": [],
			"cwd": "${workspaceRoot}",
			"environment": [],
			"externalConsole": true,
			"MIMode": "lldb",
			"breakpointers": {
				"exception": {
					"cpp_catch": "N",
					"cpp_throw": "Y"
				}
			}
		}
	},
	"attach": {
		"adapter": "CodeLLDB",
		"filetypes": [
			"cpp",
			"c",
			"objc",
			"rust"
		],
		"configuration": {
			"name": "attach",
			"type": "cppdbg",
			"request": "attach",
			"program": "${workspaceRoot}/a.out",
			"MIMode": "lldb",
			"breakpointers": {
				"exception": {
					"cpp_catch": "N",
					"cpp_throw": "Y"
				}
			}
		}
	}
}
